{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "uha-adf-d"
		},
		"copydata_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'copydata_ls'"
		},
		"copydatadb_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'copydatadb_ls'"
		},
		"lookup_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lookup'"
		},
		"copydata_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stuhad.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "uha-adf-d_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"copydata_ls_accountKey": {
						"value": "[parameters('copydata_ls_accountKey')]"
					},
					"copydatadb_ls_connectionString": {
						"value": "[parameters('copydatadb_ls_connectionString')]"
					},
					"lookup_connectionString": {
						"value": "[parameters('lookup_connectionString')]"
					},
					"copydata_ls_properties_typeProperties_url": {
						"value": "[parameters('copydata_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}